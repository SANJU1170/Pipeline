pipeline {
	agent none
		stages {
			stage ('BUILD'){
			          { git branch: 'main', url: 'https://github.com/SANJU1170/C-Projects.git' }
				agent { label 'agent1' }
				steps {
					echo "This is Build stage"
					  }
				   }  
				
			stage ('TEST'){
				agent { label 'slave1' }
				steps {
					echo "This is Test stage"
					  }
				   }  	
				
			stage ('DEPLOY'){
				agent { label 'slave2' }
				steps {
					echo "This is Deploy stage"
					  }
				   } 	
				}
	      }
