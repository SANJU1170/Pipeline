pipeline {
	agent any
		stages {
			stage ('BUILD'){
				steps {
					echo "This is Build stage"
					  }
				   }  
				
			stage ('TEST PARALLEL'){
			parallel {
				stage ('TEST IN CHROME') {
					steps {
					echo "This is test1 stage for CHROME"
					  }
					 }
			    stage ('TEST ON UC') {
				steps {
					echo "This is test2 stage for UC"  
					     }
					   }
					 } 
				  }  		
			stage ('DEPLOY'){
				steps {
					echo "This is Build stage"
					  }
				   } 	
				}
	      }
