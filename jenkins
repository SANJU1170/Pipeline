pipeline {
	agent any
		stages {
			stage ('CLONE'){
				steps { 
					echo "This is clone"
					  }
					 }
		   
		    stage ('BUILD'){
				steps {
					echo "This is Build"
					}
		         }
		
            stage ('TEST'){
				steps {
					echo "This is Test"
					}
		         }
	        }
	     }

