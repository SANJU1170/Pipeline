pipeline {
	agent any
		stages {
			stage ('BUILD'){
				steps {
					echo "This is Build stage"
					  }
				   }  
				
			stage ('TEST1'){
			parallel {
				steps {
					echo "This is test1 stage for uc"
					  }
			stage ('TEST2') {
				steps {
					echo "This is test2 stage for chrome"  
					     }
					   }
					 } 
				  }  		
			stage ('DEPLOY'){
				steps {
					echo "This is Deploy stage"
					  }
				   } 	
				}
	      }
