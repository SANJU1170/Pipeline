pipeline {
	agent any
		stages {
			stage ('CLONE <--> TEST PARALLEL'){
				parallel {
				stage ('clone-01'){
				steps { 
					echo "This is clone-01"
					  }
					 }
			     stage ('clone-02') {
				 steps {
					echo "This is Clone-02"
					}
				}
     		}
		   
		    stage ('BUILD'){
				steps {
					echo "This is Build"
					}
		         }
		     }
	     }
      }
